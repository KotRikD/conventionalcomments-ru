@import url('httpthes://fonts.googleapis.com/css?family=Lato&display=swap');

.cc-content {
  h1 {
    @apply text-2xl;
  }
  h2 {
    @apply text-xl;
  }
  h3 {
    @apply text-lg;
  }
}

.cc-speech-bubble {
  @apply border border-solid rounded py-1 px-2 relative;
  font-family: "Lato", sans-serif;
}
